	private String cinemaName;//会员账户影院
	private Integer  ckCount;//享有兑换次数
	private double  posAmt;//卖品礼包金额 
	
	@Transient
	private C0003_MEM_MEMBER member;

	/**
	 * member
	 *
	 * @return  the member
	 * @since   1.0.0
	 */
	
	public C0003_MEM_MEMBER getMember() {
		return member;
	}
	/**
	 * @param member the member to set
	 */
	public void setMember(C0003_MEM_MEMBER member) {
		this.member = member;
	}
	/**set get*/
	public String getAcCountNo() {
		return acCountNo;
